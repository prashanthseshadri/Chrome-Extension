{
  "name": "Kurukshetra 2012",
  "version": "1.3",
  "description": "This app lets you to know the latest updates happening in Kurukshetra 2012 - International Technomanagement festival of CEG, Anna University",
  "js": ["jquery.js"],
  "icons": { "128": "images/icon.png" },
  "browser_action": {
    "default_icon": "images/icon.png",
	"popup": "Home.html"
  },
  "homepage_url":"http://kurukshetra.org.in/",
  "background_page": "background.html",
  "update_url": "http://extension.kurukshetra.org.in/updates.xml",
  "options_page":"options.html",
  "permissions": [
	"notifications",
	"unlimitedStorage",
	"http://extension.kurukshetra.org.in/*.html"
	]

}
